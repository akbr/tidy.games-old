{
  "name": "vite-test",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite apps/$npm_config_app_id",
    "build": "tsc -p ./apps/$npm_config_app_id && vite build apps/$npm_config_app_id",
    "preview": "vite preview apps/$npm_config_app_id",
    "build-node": "tsc --project apps/$npm_config_app_id/tsconfig-node.json && tsc-alias -p apps/$npm_config_app_id/tsconfig-node.json",
    "heroku-postbuild": "npm run build-client --app_name=$APP_NAME && npm run build-servers"
  },
  "dependencies": {
    "@twind/preact": "^1.0.5",
    "express": "^4.17.3",
    "fast-deep-equal": "^3.1.3",
    "preact": "latest",
    "twind": "^0.16.16",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.1.5",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.18",
    "@types/ws": "^8.5.2",
    "tsc-alias": "^1.6.4",
    "typescript": "4.6.1-rc",
    "vite": "^2.8.0"
  }
}
